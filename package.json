{
  "name": "xtranslate-website",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit --watch",
    "prisma:ui": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:client": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma generate && prisma migrate deploy && prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "postinstall": "if [ \"$VERCEL\" = \"1\" ]; then npm run prisma:deploy; fi",
    "redis-dev": "redis-server",
    "update-packages": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@fluent/bundle": "^0.19.1",
    "@next/third-parties": "^15.3.4",
    "@prisma/client": "^6.10.1",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "marked": "^15.0.12",
    "next": "^15.3.4",
    "next-auth": "^5.0.0-beta.27",
    "openai": "^5.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "stripe": "^18.2.1",
    "typescript-cookie": "^1.0.6"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prisma": "^6.10.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
